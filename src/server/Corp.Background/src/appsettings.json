{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Seq": {
      "ServerUrl": "http://localhost:5341",
      "ApiKey": "1234567890"
    }
  },
  "AllowedHosts": "*",
  "CorpDbProvider": "***OVERRIDE***",
  "MyOtherDbProvider": "***OVERRIDE***",
  "HangfireDbProvider": "***OVERRIDE***",
  "IsTenantInterceptorEnabled": true,
  "ConnectionStrings": {
    "SqliteCorpDbContextConnection": "***SECRET***",
    "NpgsqlCorpDbContextConnection": "***SECRET***",
    "SqliteMyOtherDbContextConnection": "***SECRET***",
    "NpgsqlMyOtherDbContextConnection": "***SECRET***",
    "SqliteDefaultHangfireConnection": "***SECRET***",
    "NpgsqlDefaultHangfireConnection": "***SECRET***"
  },
  "IdentityOptions": {
    "SignIn": {
      "RequireConfirmedEmail": true
    },
    "Password": {
      "RequiredLength": 8
    }
  },
  "AppRateLimit": {
    "PermitLimit": 100
  },
  "AuthSendgridOptions": {
    "SendGridKey": "***SECRET***"
  }
}
